get_filename_component(MojaFlintConfiguration_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${MojaFlintConfiguration_CMAKE_DIR})

find_dependency(Poco 1.9.2 REQUIRED Foundation JSON)
list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET Moja::FlintConfiguration)
    include("${MojaFlintConfiguration_CMAKE_DIR}/MojaFlintConfigurationTargets.cmake")
endif()

set(MojaFlintConfiguration_LIBRARIES Moja::FlintConfiguration)