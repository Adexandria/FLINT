get_filename_component(MojaModulesZipper_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${MojaModulesZipper_CMAKE_DIR})

find_dependency(PostgreSQL REQUIRED)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET Moja::ModulesZipper)
    include("${MojaModulesZipper_CMAKE_DIR}/MojaModulesZipperTargets.cmake")
endif()

set(MojaModulesZipper_LIBRARIES Moja::ModulesZipper)