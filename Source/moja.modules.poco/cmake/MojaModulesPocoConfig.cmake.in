get_filename_component(MojaModulesPoco_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${MojaModulesPoco_CMAKE_DIR})

find_dependency(PostgreSQL REQUIRED)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET Moja::ModulesPoco)
    include("${MojaModulesPoco_CMAKE_DIR}/MojaModulesPocoTargets.cmake")
endif()

set(MojaModulesPoco_LIBRARIES Moja::ModulesPoco)