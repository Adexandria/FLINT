get_filename_component(MojaModulesLibPQ_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${MojaModulesLibPQ_CMAKE_DIR})

find_dependency(PostgreSQL REQUIRED)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET Moja::ModulesLibPQ)
    include("${MojaModulesLibPQ_CMAKE_DIR}/MojaModulesLibPQTargets.cmake")
endif()

set(MojaModulesLibPQ_LIBRARIES Moja::ModulesLibPQ)